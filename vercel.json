{
  "version": 2,
  "builds": [
    {
  src": "api/*.mjs",
  "use": "@vercel/node"
  },
    {
      "src": "api/*.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
  {
    "src": "/api/consumo-energetico",
    "dest": "/api/consumo-server.mjs"
  },
  {
    "src": "/api/huella-carbono",
    "dest": "/api/footprint-server.mjs"
  },
  {
    "src": "/api/produccion-solar",
    "dest": "/api/solar-server.mjs"
  },
  {
    "src": "/(.*)",
    "dest": "/public/$1"
  }
]

}
